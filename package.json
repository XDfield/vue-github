{
  "name": "vue-github",
  "version": "1.0.0",
  "description": "The Github base on nuxt.",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "npm run generate:gh-pages; gh-pages -d dist"
  },
  "keywords": [],
  "author": "DoSun",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "ajv": "^6.5.3",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-beta",
    "eslint": "^4.19.1",
    "gh-pages": "^2.0.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.3",
    "nuxt": "^2.0.0",
    "sass-loader": "^7.1.0",
    "vue-awesome": "^3.1.2"
  }
}
